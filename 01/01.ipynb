   "source": [
    "with open('01.txt', 'r') as f:\n",
    "    data = f.read().split('\\n')\n",
    "\n",
    "l1: tuple[int]\n",
    "l2: tuple[int]\n",
    "\n",
    "l1, l2 = zip(*[[int(n) for n in x.split()] for x in data])\n",
    "t1: int = sum([abs(x-y) for (x, y) in zip(sorted(l1), sorted(l2))])\n",
    "t2: int = sum([x*l2.count(x) for x in l1])\n"
   ]
  }
